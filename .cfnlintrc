ignore_checks:
  # W3037: IAM action/service validation often out of date with new AWS APIs
  # Suppressing for:
  # - elasticloadbalancingv2:* (valid ELBv2 service for ALB/NLB)
  # - elasticfilesystem:PutFileSystemProtection (valid EFS API added 2023)
  - W3037
  
  # W3045: AccessControl is legacy property
  # We've migrated to object_ownership but warning persists in generated templates
  # This warning will resolve once the stack is redeployed with the new configuration
  - W3045
  
  # E3030: Lambda runtime validation out of date
  # Suppressing for:
  # - python3.14 (valid Lambda runtime, GA December 2024)
  # - nodejs22.x (valid Lambda runtime, GA November 2024)
  - E3030
  
  # E3690: Aurora MySQL engine version validation out of date
  # Suppressing for:
  # - 8.0.mysql_aurora.3.11.0 (valid Aurora MySQL version released 2024)
  - E3690
  
  # W3005: Redundant dependency warnings
  # CDK automatically manages dependencies via GetAtt/Ref
  # These explicit dependencies are intentional for deletion ordering
  - W3005
